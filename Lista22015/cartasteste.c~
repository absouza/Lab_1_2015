#include <stdio.h>

int main(){
    int a, b, c,a1 ,b1 ,c1,aux;
    scanf("%d %d %d" , &a, &b, &c);
    a1 = a/2;
    b1 = b/3;
    c1 = c/5;

  if (a1 > b1){
        aux = a1;
        a1 = b1;
        b1 = aux;
    }
    if (a1 > c1){
        aux = a1;
        a1 = c1;
        c1 = aux;
    }
    if (b1 > c1){
        aux = b1;
        b1 = c1;
        c1 = aux;
    }
    printf("%d\n" , a1);

return 0;
}


